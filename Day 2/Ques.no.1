bool check(string pattern, string word)
{
    if (pattern.length() != word.length())
     {
        return false;
     }
    int l = word.length();
    char ch[l] = { 0 };
    for (int i = 0; i < l; i++) {
        if (ch[pattern[i]] == 0)
         {
            ch[pattern[i]] = word[i];
         }
        else if (ch[pattern[i]] != word[i])
            return false;
    }
 
    return true;
}
 
