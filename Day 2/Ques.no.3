#include <iostream.h>
using namespace std;
int onesCount(int M[][], int r, int c)
{
     int count=0;
     for(int i=0; i<r; i++)
     {
        for(int j=0; j<c; j++)
        {
             if(M[i][j]==1)
              count++;
        }
     }
     return count;
}
void main()
{
cout<< "enter the no. of rows";
cin>> r;
cout<< "enter the no. of column";
cin>> c;
cout<< "enter element of an array";
  for(int i=0; i<r; i++)
   {
        for(int j=0; j<c; j++)
        {
           cout<< "M[i][j] = ";
           cin>> M[i][j];
        }
   }
}
