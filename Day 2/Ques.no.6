#include<iostream.h>
using namespace std;
void main()
{
  int n, i, positiveCount=0, negSum=0;
  cout<< "enter the size of array";
  cin>>n;
  int a[n];
  cout<<"enter element of an array";
  for(i=0; i<n; i++)
    {
        cin>>a[i];
    }
  for(i=0; i<n;i++)
   {
      if(a[i]>=0)
       {
          positiveCount++;
       }
      else
       { 
          negSum+= a[i];
       }
   }
  cout<<"result = "<< "[ "<< positiveCount << negSum <<" ]";
}
