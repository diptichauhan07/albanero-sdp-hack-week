#include<iostream.h>
#include<string.h>
using namespace std;
void main()
{
 int n;
 cout<<"enter the size of string";
 cin>> n;
 string str;
if (n % 2 == 0)
          {
                for(int i = 0; i < n - 1; i++)
                {
                    str.Append("x");
                }
                  str.Append("y");
           }
            else
            {
                for (int i = 0; i < n; i++)
                    str.Append("z");
            }
      cout<< str;      
}
