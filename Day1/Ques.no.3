#include <iostream>
using namespace std;
void sum( int arr[] , int n)
{
  int sum = 0;
  for( int i=0; i < n; i++)
  {
     sum+= arr[i];
  }
  if( sum%2==0 )
  {
      cout << "even";
  }
  cout << "odd";
}

int main()
{
  int arr[];
  int n = sizeof( arr )/ sizeof(arr[0]);
  cout<< "Enter the element in an array  ";
  for(int i = 0; i<n;i++)
  {
     cin>> arr[i];
  }
  sum(arr, n);
  return 0;
}
