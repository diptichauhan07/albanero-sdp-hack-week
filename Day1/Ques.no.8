#include<iostream.h>
using namespace std;
 
int maxProfit(int price[], int start, int end)
{
    if (end <= start)
        return 0;
    int profit = 0;
    for (int i = start; i < end; i++) {
        for (int j = i + 1; j <= end; j++) {
            if (price[j] > price[i]) {
                int curprofit = price[j] - price[i]
                                  + maxProfit(price, start, i - 1)
                                  + maxProfit(price, j + 1, end);
                profit = max(profit, curprofit);
            }
        }
    }
    return profit;
}
 
int main()
{
    int price[], n:
    cout<< "enter the size of array";
    cin>>n;
    for(int i=0; i<n; i++)
    {
        cin>> price[i];
    }
    maxProfit(price, 0, n - 1);
 
    return 0;
}
