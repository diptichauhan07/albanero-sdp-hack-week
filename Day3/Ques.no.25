#include <iostream.h>
using namespace std;
void main()
 {
    string str;
    cout<<"enter the string" << endl;
    cin>> str;
    for (int i = 0; i < str.length; i++)
      {
        str[i] = tolower(str[i]);
      }
    sort(str.begin(), str.end());
  
    for (int i = 0; i < str.length; i++) 
      {
          if (str[i] == str[i + 1])
              cout<<"False";
      }
    cout<<"True";
}
    
