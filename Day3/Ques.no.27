#include<iostream.h>
using namespace std;
void arrayOfMultiples(int num, int lenght)
 {
     int arr[lenght];
     for(int i=0; i<lenght; i++)
      {
         cout<< arr[i]=num*i;
      }
 }
void main()
 {
    int num, lenght;
