#include<iostream.h>
using namespace std;
void main()
 {
    int arr[2], s1=0, s2=0;
    cout<<"enter the value in array:"
    for(i=0; i<2; i++)
     {
        cin>>arr[i];
     }
    while (arr[0]!= 0)
    {
        s1 = s1 + (arr[0]%10);
        arr[0] = arr[0]/ 10;
    }
     while (arr[1]!= 0)
    {
        s2 = s2 + (arr[1]%10);
        arr[1] = arr[1]/ 10;
    }
    if(s1==s2)
     cout<<"true";
    else
     cout<<"false";
 }
