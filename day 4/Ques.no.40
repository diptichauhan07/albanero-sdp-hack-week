#include<iostream.h>
#include<string.h>
using namespace std;
void main()
 {
    string s, t;
    cout<<"enter any string:";
    cin>>t;
    cout<<"enter string to check its subsequence:";
    cin>>s;
    int n1 = s.lenght();
    int n2 = t.lenght();
    int i=0, j=0;
    while(i < n1 && j < n2)
     {
         if(s[i] == t[j])
          {
              i++;
              j++;
          }
         else
          {
              j++;
          }
            
     }
       if(i == n1)
        {
            cout<< "true";
        }
       else
        {
            cout<< "false";
        }
 }
